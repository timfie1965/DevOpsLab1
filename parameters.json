{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "value": "eastus"
        },
        "virtualNetworkName": {
            "value": "OnPremVnet"
        },
        "resourceGroup": {
            "value": "OnPrem"
        },
        "addressSpaces": {
            "value": [
                "192.168.0.0/16"
            ]
        },
        "ipv6Enabled": {
            "value": false
        },
        "subnetCount": {
            "value": 3
        },
        "subnet0_name": {
            "value": "MgmtSubnet"
        },
        "subnet0_addressRange": {
            "value": "192.168.1.0/24"
        },
        "subnet1_name": {
            "value": "DBsubnet"
        },
        "subnet1_addressRange": {
            "value": "192.168.2.0/24"
        },
        "subnet2_name": {
            "value": "WebSubnet"
        },
        "subnet2_addressRange": {
            "value": "192.168.3.0/24"
        },
        "ddosProtectionPlanEnabled": {
            "value": false
        },
        "firewallEnabled": {
            "value": false
        },
        "bastionEnabled": {
            "value": false
        }
    }
}